#It's a bit trickier for command blocks. First, we gonna place the command block and have it run this function next tick.
execute if score mainhand temp matches 2 run tag @s add spawnerlore
execute if score mainhand temp matches 2 run setblock 7777 57 7777 air
execute if score mainhand temp matches 2 run setblock 7777 58 7777 air
execute if score mainhand temp matches 2 run setblock 7777 57 7777 repeating_command_block[conditional=false,facing=up]{Command:"/execute as @p[tag=spawnerlore] at @s run function tools:spawnerlore",TrackOutput:0b,powered:0b,auto:1b,conditionMet:1b} destroy
execute if score mainhand temp matches 2 run setblock 7777 58 7777 repeating_command_block[conditional=false,facing=up]{Command:"/say test",TrackOutput:0b,powered:0b,auto:1b,conditionMet:1b} destroy
execute if score mainhand temp matches 2 run data modify block 7777 58 7777 Command set from storage spawnerlore:item Item.tag.BlockEntityTag.Command

#Gotta wait ANOTHER tick ;-;
execute if score mainhand temp matches 3 run setblock 7777 57 7777 repeating_command_block[conditional=false,facing=up]{Command:"/execute as @p[tag=spawnerlore] at @s run function tools:spawnerlore",TrackOutput:0b,powered:0b,auto:1b,conditionMet:1b} destroy
execute if score mainhand temp matches 3 run tag @s add spawnerlore2

#Now that we know it ran, let's grab the spawner.
execute if score mainhand temp matches 4 run tag @a remove spawnerlore
execute if score mainhand temp matches 4 run tag @a remove spawnerlore2
execute if score mainhand temp matches 4 run data modify block 7777 60 7777 Items[0].tag.BlockEntityTag set from block 7777 58 7777
execute if score mainhand temp matches 4 run setblock 7777 58 7777 air
execute if score mainhand temp matches 4 run setblock 7777 57 7777 air

